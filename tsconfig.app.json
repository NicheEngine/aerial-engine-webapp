{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": [
    "env.d.ts",
    "vite.env.d.ts",
    "types/*",
    "types/**/*.d.ts",
    "src/*",
    "src/**/*",
    "src/**/*.vue",
    "src/**/*.ts",
    "src/**/*.d.ts",
    "src/**/*.tsx",
    "src/components/*",
    "src/components/**/*",
    "src/components/**/*.vue",
    "src/components/**/*.ts",
    "src/components/**/*.d.ts",
    "src/components/**/*.tsx",
    "src/routers/*",
    "src/routers/**/*",
    "src/routers/**/*.vue",
    "src/routers/**/*.ts",
    "src/routers/**/*.d.ts",
    "src/routers/**/*.tsx",
    "src/stores/*",
    "src/stores/**/*",
    "src/stores/**/*.vue",
    "src/stores/**/*.ts",
    "src/stores/**/*.d.ts",
    "src/stores/**/*.tsx",
    "src/styles/*",
    "src/styles/**/*",
    "src/styles/**/*.vue",
    "src/styles/**/*.ts",
    "src/styles/**/*.d.ts",
    "src/styles/**/*.tsx",
    "src/widgets/*",
    "src/widgets/**/*",
    "src/widgets/**/*.vue",
    "src/widgets/**/*.ts",
    "src/widgets/**/*.d.ts",
    "src/widgets/**/*.tsx",
    "src/views/*",
    "src/views/**/*",
    "src/views/**/*.vue",
    "src/views/**/*.ts",
    "src/views/**/*.d.ts",
    "src/views/**/*.tsx",
    "src/pages/*",
    "src/pages/**/*",
    "src/pages/**/*.vue",
    "src/pages/**/*.ts",
    "src/pages/**/*.d.ts",
    "src/pages/**/*.tsx",
    "types/*",
    "types/**/*",
    "types/**/*.d.ts",
    "types/**/*.ts",
    "plugins/*",
    "plugins/**/*",
    "plugins/**/*.ts",
    "plugins/**/*.d.ts",
    "scripts/*",
    "scripts/**/*",
    "scripts/**/*.ts",
    "scripts/**/*.d.ts"
  ],
  "exclude": [
    "src/**/__tests__/*",
    "node_modules",
    "tests/server/**/*.ts",
    "dist",
    "**/*.js"
  ],
  "compilerOptions": {
    "tsBuildInfoFile": "node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "target": "ESNext",
    "module": "ESNext",
    "noImplicitAny": false,
    "allowJs": true,
    "strict": true,
    "jsx": "preserve",
    "include": ["src/**/*.tsx", "src/**/*.vue"],
    "importHelpers": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "types": [
      "node",
      "vite/client"
    ],
    "paths": {
      "vue-i18n": [
        "vue-i18n/dist/vue-i18n.cjs.js"
      ],
      "@aps/*": [
        "src/apis/*"
      ],
      "@ass/*": [
        "src/assets/*"
      ],
      "@cps/*": [
        "src/components/*"
      ],
      "@dts/*": [
        "src/directives/*"
      ],
      "@ems/*": [
        "src/enums/*"
      ],
      "@hks/*": [
        "src/hooks/*"
      ],
      "@hts/*": [
        "src/https/*"
      ],
      "@lys/*": [
        "src/layouts/*"
      ],
      "@lcs/*": [
        "src/locales/*"
      ],
      "@lgs/*": [
        "src/logics/*"
      ],
      "@mds/*": [
        "src/models/*"
      ],
      "@pgs/*": [
        "src/pages/*"
      ],
      "@rts/*": [
        "src/routers/*"
      ],
      "@sts/*": [
        "src/settings/*"
      ],
      "@srs/*": [
        "src/stores/*"
      ],
      "@sls/*": [
        "src/styles/*"
      ],
      "@tps/*": [
        "src/types/*"
      ],
      "@uts/*": [
        "src/utils/*"
      ],
      "@vws/*": [
        "src/views/*"
      ],
      "@wds/*": [
        "src/wds/*"
      ],
      "@/*": [
        "src/*"
      ],
      "#/*": [
        "*"
      ]
    }
  }
}
